{
  "name": "app-template",
  "version": "6.1.1",
  "description": "Angular app template",
  "main": "./build/app.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/paradox41/app-template"
  },
  "author": "Will <will.soto9+github@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paradox41/app-template/issues"
  },
  "keywords": [
    "angular",
    "gulp",
    "template"
  ],
  "scripts": {
    "lint": "gulp lint",
    "test": "gulp test:once",
    "build": "npm install && gulp build",
    "docs": "./node_modules/.bin/jsdoc -c jsdoc.conf.json"
  },
  "git": {
    "scripts": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test",
      "commit-msg": "./hooks/validate-commit-msg.js $1"
    }
  },
  "homepage": "https://github.com/paradox41/app-template",
  "devDependencies": {
    "aliasify": "^1.7.2",
    "angular-mocks": "^1.3.16",
    "babel-core": "^6.0.20",
    "babel-preset-es2015": "^6.0.15",
    "babelify": "^7.2.0",
    "browser-sync": "^2.7.5",
    "browserify": "^12.0.1",
    "browserify-shim": "^3.8.10",
    "chai": "^3.2.0",
    "del": "^2.0.2",
    "git-scripts": "^0.2.1",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-bump": "^1.0.0",
    "gulp-compile-handlebars": "^0.5.0",
    "gulp-conventional-changelog": "^0.7.0",
    "gulp-debug": "^2.0.0",
    "gulp-eslint": "^1.0.0",
    "gulp-filter": "^3.0.0",
    "gulp-gh-pages": "^0.5.2",
    "gulp-git": "^1.2.4",
    "gulp-if": "^2.0.0",
    "gulp-minify-css": "^1.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.0",
    "gulp-rev": "^6.0.1",
    "gulp-sass": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tag-version": "^1.1.0",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.1",
    "jsdoc": "^3.4.0",
    "karma": "^0.13.3",
    "karma-browserify": "^4.2.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.0",
    "karma-coveralls": "^1.1.2",
    "karma-mocha": "^0.2.0",
    "karma-spec-reporter": "0.0.22",
    "minimist": "^1.1.1",
    "mocha": "^2.2.5",
    "partialify": "^3.1.3",
    "run-sequence": "^1.0.1",
    "sassdoc": "^2.1.15",
    "stripify": "^3.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^3.4.0"
  },
  "browser": {
    "app": "./app/app.js",
    "angular-ui-router": "./node_modules/angular-ui-router/release/angular-ui-router.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "aliasify"
    ]
  },
  "aliasify": {
    "aliases": {
      "common": "./app/common"
    }
  },
  "browserify-shim": {
    "angular": {
      "exports": "angular"
    },
    "angular-ui-router": {
      "depends": [
        "angular"
      ]
    }
  },
  "dependencies": {
    "angular": "^1.4.5",
    "angular-ui-router": "^0.2.15",
    "bootstrap-sass": "^3.3.5",
    "font-awesome": "^4.4.0",
    "lodash": "^3.10.1"
  }
}
